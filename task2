		//Check if input is digit → then check if it is even digit.
		System.out.println("enter no");
		int no=scan.nextInt();
		if(no>0){
		if(no%2==0){
		System.out.println("even");
		}
		else{
		System.out.println("not even");
		}
		}
		else{
		System.out.println("not digit");
		}
		//Check if password length ≥ 8 → then check if it contains a digit.
		System.out.println("enter pass");
		int pass=scan.nextInt();
		if(pass.length()>=8){
		if(pass>='0' ){
		System.out.println("digit");
		}
		else{
		System.out.println(" not digit");
		}
		}
		else{
		System.out.println(" len less than 8");
		}
		//Check if number > 1 → then check if prime.
		//no div by 1 and itself
		System.out.println("enter no");
		int no=scan.nextInt();
		if(no>1) {
			
			if(no%2==0) {
				System.out.println("prime");
			}
			else {
				System.out.println("not prime");
			}
		}
		else {
			System.out.println("less than 1");
		}

		//Check if marks ≥ 35 → then check if marks ≥ 90 (Topper).
		System.out.println("enter mark");
		int mark=scan.nextInt();
		if(mark>=35){
		if(mark>=90){
		System.out.println("topper");
		}
		else{
		System.out.println("avg");
		}
		}
		else{
		System.out.println("fail");
		}
		//Check if temperature > 0 → then check if less than 20 (Cold category).
		System.out.println("enter temp");
		int temp=scan.nextInt();
		if(temp>0){
		if(temp<=20){
		System.out.println("freeze");
		}
		else{
		System.out.println("hot");
		}
		}
		else{
		System.out.println("less than 0");
		}

		//Check if account balance > 0 → then check if withdrawal amount ≤ balance.
		
		int balance=5000;
		if(balance>0){
		System.out.println("enter func");
		String func=scan.nextLine();
		if(func.equals("withdraw"){
		System.out.println("enter amount");
		int amount=scan.nextInt();
		if(amount<=balance){
		balance=balance-amount;
		System.out.println("amount withdraw");
		}
		else{
		System.out.println("INSUFFECIENT");
		}
		}
		else{
		System.out.println("another func");
		}}
		else{
		System.out.println("insuffecient baLance");
		}
		}
		
	}
}
