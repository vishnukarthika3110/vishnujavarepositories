		package corejava;

import java.util.Scanner;

public class Elseif {

	public static void main(String[] args) {
	//season based on month number
	Scanner scan=new Scanner(System.in);
	System.out.println("enter num");
	int num=scan.nextInt();
	if(num==1 || num==2 || num==12) {
		System.out.println("winter");
	}
	else if(num==3 || num==4 || num==5) {
		System.out.println("summer");
	}
	else if(num==6 || num==7 || num==8) {
		System.out.println("autumm");
	}
	else if(num==9 || num==10 || num==11) {
		System.out.println("rainy");
	}
	else {
		System.out.println("you entered wrong number");
	}
	//check type of quadrilateral
	System.out.println("enter num");
	int a=scan.nextInt();
	int b=scan.nextInt();
	int c=scan.nextInt();
	int d=scan.nextInt();
	
	int a1=scan.nextInt();
	int a2=scan.nextInt();
	int a3=scan.nextInt();
	int a4=scan.nextInt();
	
	if((a==b && a==c && a==d) && (a1==a2  && a1==a3 && a1==a4) ) {
		System.out.println("square");
	}
	else if((a==c && b==d ) &&(a1==a2  && a1==a3 && a1==a4)) {
		System.out.println("rectangle");
	}
	else if((a==b && a==c && a==d) && (a1!=90  && a2!=90 && a3!=90 && a4!=90)) {
		System.out.println("rhombus");
	}
	else if((a==c && b==d) && (a1==a3 && a2==a4)) {
		System.out.println("parallelogram");
	}
	else if((((a==c && b!=d)|| (a!=b && c==d) ) && ((a1==a3 && a2!=a4) ||(a1!=a2  && a3==a4)  ))) {
		System.out.println("trapezium");
	}
	else if(((a==b && c==d && a!=c)||(a==d && b==c && a!=b) ) &&(a==c || b==d)){
		System.out.println("kite");
	}
	else{
	System.out.println("invalid");
	}
